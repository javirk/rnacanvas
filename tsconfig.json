{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "strictBindCallApply": true,
    "strictFunctionTypes": true,
    "strictNullChecks": true,
    "strict": true,
    "moduleResolution": "Node",
    "allowSyntheticDefaultImports": false,
    "baseUrl": "./",
    "paths": {
      "Array/*": ["src/array/*"],
      "Conditions/*": ["src/conditions/*"],
      "Draw/*": ["src/draw/*"],
      "Export/*": ["src/export/*"],
      "Foreign/*": ["src/foreign/*"],
      "Forms/*": ["src/forms/*"],
      "History/*": ["src/history/*"],
      "Infobar/*": ["src/infobar/*"],
      "Math/*": ["src/math/*"],
      "Menu/*": ["src/menu/*"],
      "Parse/*": ["src/parse/*"],
      "Partners/*": ["src/partners/*"],
      "Refresh/*": ["src/refresh/*"],
      "Settings/*": ["src/settings/*"],
      "Time/*": ["src/time/*"],
      "Undo/*": ["src/undo/*"],
      "Utilities/*": ["src/utilities/*"],
      "Values/*": ["src/values/*"],
      "App": ["src/App"],
    },
    "module": "es6",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
  }
}
