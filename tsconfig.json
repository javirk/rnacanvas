{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "strictBindCallApply": true,
    "strictFunctionTypes": true,
    "strictNullChecks": true,
    "moduleResolution": "Node",
    "module": "es6",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
  }
}
